  GET(RecentQueue,1)

  do ProjectLoad
  do DictionaryLoad
  do DiagramLoad
  do FileLoad

  ?list1{prop:selected} = 1
  ?list2{prop:selected} = 1
  ?list3{prop:selected} = 1
  ?list4{prop:selected} = 1
  ?list5{prop:selected} = 1
