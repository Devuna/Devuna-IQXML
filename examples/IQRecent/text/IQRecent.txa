PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
FileSave    ROUTINE

   REMOVE(FullName)
   if errorcode()
      MESSAGE('The Recent List file could not be written at this time.||Error: ' & ERROR(),'Error',ICON:Exclamation)
      EXIT
   END

   IF XML:CreateXMLFILE(FullName,'')
      MESSAGE('Error occurred creating file.||Error: ' & XML:GetError())
   ELSE
      XML:CreateParent('Properties');xml:AddParent()
      xml:CreateParent('Project')
      FinalValue = ''
      LOOP I = 1 TO RECORDS(EndFileQueue)
        GET(EndFileQueue,I)
        FinalValue = FinalValue & clip(endfilequeue.FileName) & ','
      END
      xml:CreateAttribute('Value',FinalValue)
      xml:AddParent(True)

      xml:CreateParent('Dictionary')
      FinalValue = ''
      LOOP I = 1 TO RECORDS(EndDictQueue)
        GET(EndDictQueue,I)
        FinalValue = FinalValue & clip(EndDictQueue.FileName) & ','
      END
      xml:CreateAttribute('Value',FinalValue)
      xml:AddParent(True)

      xml:CreateParent('Diagram')
      FinalValue = ''
      LOOP I = 1 TO RECORDS(EndDiagQueue)
        GET(EndDiagQueue,I)
        FinalValue = FinalValue & clip(EndDiagQueue.FileName) & ','
      END
      xml:CreateAttribute('Value',FinalValue)
      xml:AddParent(True)

      XML:CreateParent('Properties')
      XML:CreateAttribute('name','Table')
      XML:AddParent()
      LOOP I = 1 TO RECORDS(FileQueue)
         GET(FileQueue,I)
         XML:CreateParent('Properties')
         xml:CreateAttribute('name',FQ:PropertiesName)
         xml:AddParent()
         xml:AddElement('HasUID',clip(FQ:HasUIDvalue))
         xml:AddElement('HasPWD',clip(FQ:HasPWDvalue))
         xml:AddElement('FileDriver',clip(FQ:FileDrivervalue))
         xml:AddElement('DriverString',clip(FQ:DriverStringValue))
         xml:CloseParent()
      end
      XML:CloseParent()

      xml:CreateParent('File')
      FinalValue = ''
      LOOP I = 1 TO RECORDS(EndSrcQueue)
        GET(EndSrcQueue,I)
        FinalValue = FinalValue & clip(EndSrcQueue.FileName) & ','
      END
      xml:CreateAttribute('Value',FinalValue)
      xml:AddParent(True)

      xml:CloseXMLFile(True)
  END
