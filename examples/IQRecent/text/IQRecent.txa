PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
Tweaker2    ROUTINE

  if Tweak_New=false and Tweak_Close=false then exit.

  PathName = GETREG( REG_LOCAL_MACHINE,'SOFTWARE\SoftVelocity\Clarion10','root')
  Glo:filename1 = CLIP(PathName) & '\bin\addins\Clarion.addin'
  Glo:filename2 = 'Clarion.addin'

  InFileItem = False
  InNewMenuItem = False
  InCloseMenuitem = False

  OPEN(TextFile1)
  IF ERRORCODE()
        MESSAGE('Could not open the ' & glo:filename1 & ' file.||Error: ' & ERROR(),'Error Occurred',icon:exclamation)
        EXIT
  END

  REMOVE(TEXTFILE2)
  CREATE(TEXTFILE2)
  OPEN(TEXTFILE2)
  if errorcode()
        MESSAGE('Could not open the work file.||Error: ' & ERROR(),'Error Occurred',icon:exclamation)
        EXIT
  END

  SET(TEXTFILE1)
  LOOP
    NEXT(TEXTFILE1)
    IF ERRORCODE() THEN BREAK.

    TXT2:TEXTLINE = TXT1:TEXTLINE

    IF INSTRING('<!-- Main Menu -->',TXT1:TEXTLINE,1,1)
       InFileItem = True
    end

    if InFileItem = True
       if instring('(IQ)',txt1:textline,1,1)        ! skip all already changed
       else
           if Tweak_New=True and instring('<MenuItem id = "New" ',txt1:textline,1,1) > 0
              txt2:textline = '<!-- (IQ)' & CLIP(txt1:textline) & ' -->'
              InNewMenuItem = True
           else
              if Tweak_Close=True and instring('<MenuItem id = "Close" ',txt1:textline,1,1)
                 !Do AddSep
                 txt2:textline = '<!-- (IQ)' & CLIP(txt1:textline) & ' -->'
                 InCloseMenuItem = True
              else
                   if InNewMenuItem = True
                      if instring('label',txt1:textline,1,1)
                         i = instring('= "',txt1:textline,1,1)
                         if i
                             txt2:textline = sub(txt1:textline,1,i+2) & 'New ' & clip(sub(txt1:textline,i+3,1000))
                         end
                      end
                      if instring('</MenuItem>',txt1:textline,1,1)
                         Do AddSep
                         txt2:textline = '<!-- (IQ)' & CLIP(txt1:textline) & ' -->'
                         InNewMenuItem = False
                      end
                   end
                   if InCloseMenuItem = True
                      if instring('label',txt1:textline,1,1)
                         i = instring('= "',txt1:textline,1,1)
                         if i
                             txt2:textline = sub(txt1:textline,1,i+2) & 'Close ' & clip(sub(txt1:textline,i+3,1000))
                         end
                      end
                      if instring('<MenuItem id = "CloseFile"',txt1:textline,1,1)
                          txt2:textline = chr(9) & chr(9) & '<MenuItem id = "Close"'
                      else
                          if instring('</MenuItem>',txt1:textline,1,1)
                            txt2:textline = '<!-- (IQ)' & CLIP(txt1:textline) & ' -->'
                            InCloseMenuItem = False
                        end
                      end
                   end
               end
            end
        end
    end

    APPEND(TEXTFILE2)
  end

  CLOSE(TEXTFILE1)
  CLOSE(TEXTFILE2)

  REMOVE(CLIP(PATHNAME) & '\Clarion.Addin.Old')    ! remove old file
  RENAME(TEXTFILE1,'Clarion.Addin.Old')             ! make backup of current
  COPY(glo:filename2,glo:filename1)               ! copy in new file




