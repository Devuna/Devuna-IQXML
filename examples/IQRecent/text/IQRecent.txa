LoadRecent  ROUTINE

  FREE(EndFileQueue)
  FREE(FileQueue)
  FREE(RecentQueue)
  FREE(EndDictQueue)
  FREE(EndDiagQueue)
  FREE(EndSrcQueue)
  CLEAR(EndFileQueue)
  CLEAR(FileQueue)
  CLEAR(RecentQueue)

  PathName = GETREG( REG_CURRENT_USER,'Software\Microsoft\Windows\CurrentVersion\Explorer\Shell Folders','AppData')
  PathName = PathName & '\SoftVelocity\Clarion\10.0'
  FileName = 'RecentOpen.xml'
  FullName = PathName & '\' & FileName

  if not xml:LoadFromFile(FullName)
    xml:QualifyFieldSet('value')
    Recs = XML:LoadQueue(RecentQueue,True,True)
  END

  xml:GotoTop
  if not xml:FindNextNode('Properties','Properties')
     if not xml:GotoChild()
        xml:QualifyFieldSet('Name')
        Recs = XML:LoadQueue(FileQueue,True,True)
     end
  end

