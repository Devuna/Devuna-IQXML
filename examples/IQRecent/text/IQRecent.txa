PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
Tweaker1    ROUTINE

  if Tweak_AlignMenu=false then exit.

  PathName = GETREG( REG_LOCAL_MACHINE,'SOFTWARE\SoftVelocity\Clarion10','root')
  Glo:filename1 = CLIP(PathName) & '\bin\addins\backendbindings\Clarionbinding\Common\CommonAddin.addin'
  Glo:filename2 = 'CommonAddIn.addin'

  InMenuItem = False

  OPEN(TextFile1)
  IF ERRORCODE()
        MESSAGE('Could not open the ' & glo:filename1 & ' file.||Error: ' & ERROR(),'Error Occurred',icon:exclamation)
        EXIT
  END

  REMOVE(TEXTFILE2)
  CREATE(TEXTFILE2)
  OPEN(TEXTFILE2)
  if errorcode()
        MESSAGE('Could not open the work file.||Error: ' & ERROR(),'Error Occurred',icon:exclamation)
        EXIT
  END

  SET(TEXTFILE1)
  LOOP
    NEXT(TEXTFILE1)
    IF ERRORCODE() THEN BREAK.

    TXT2:TEXTLINE = TXT1:TEXTLINE

    IF INSTRING('<MenuItem id = "AlignMenu" label',TXT1:TEXTLINE,1,1)
       IF INSTRING('<!--',TXT1:TEXTLINE,1,1) = 0
           TXT2:TEXTLINE = '<!-- (IQ)' & LEFT(CLIP(TXT1:TEXTLINE)) & ' -->'
           InMenuItem = True
       END
    ELSE
       if InMenuItem = True
          if Tweak_UseAlign = True and instring('label',txt1:textline,1,1)
             i = instring('= "',txt1:textline,1,1)
             if i
                txt2:textline = sub(txt1:textline,1,i+2) & 'Align ' & clip(sub(txt1:textline,i+3,1000))
             end
          else
            IF INSTRING('</MenuItem>',TXT1:TEXTLINE,1,1)
               IF INSTRING('<!--',TXT1:TEXTLINE,1,1) = 0
                   Do AddSep
                   TXT2:TEXTLINE = '<!-- (IQ)' & LEFT(CLIP(TXT1:TEXTLINE)) & ' -->'
                   InMenuItem = False
               end
            END
          END
        END
    END
    APPEND(TEXTFILE2)
  end

  CLOSE(TEXTFILE1)
  CLOSE(TEXTFILE2)

  REMOVE(CLIP(PATHNAME) & '\CommonAddin.Addin.Old')    ! remove old file
  RENAME(TEXTFILE1,'CommonAddin.AddIn.Old')             ! make backup of current
  COPY(glo:filename2,glo:filename1)               ! copy in new file






