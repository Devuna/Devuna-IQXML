[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
  If ~Xml:LoadFromFile('AddressExample.xml',False)

       ! Walk through the XML
       ! --------------------
       XML:GotoTop()
       if XML:FindNextNode('Invoices','Invoice')
          stop('Error occurred in FindNextNode')
       else
          xml:debugqueue('This is where I think i should be')
          if xml:GotoParent()
             stop('Error occurred in GotoParent (Invoices)')  ! Invoices
          else
             if xml:gotoParent()                   ! customer
                stop('Error occurred in GotoParent (Customer)')
             else
                if Xml:GotoSibling()
                   stop('Error occurred in GotoSibling (Customer)')
                else
                   if xml:GotoChild()
                      stop('Error occurred in GotoChild (PersonName)')
                   else
                      If ~xml:ReadCurrentRecord(MyQueue,MyData)
                         Loop I = 1 to records(MyQueue)
                             Get(MyQueue,I)
                             Node = clip(Node) & chr(60) & clip(que:elementname) & chr(62)
                         End
                         Message('Currently at: ' & clip(Node) & '||Value: ' & clip(MyData),'Should be on "Robert"')
                         FREE(myqueue);CLEAR(MYQUEUE)
                         NODE = ''
                         MYDATA = ''
                         If ~xml:ReadNextRecord(MyQueue,MyData)
                            Loop I = 1 to records(MyQueue)
                                Get(MyQueue,I)
                                Node = clip(Node) & chr(60) & clip(que:elementname) & chr(62)
                            End
                            Message('Currently at: ' & clip(Node) & '||Value: ' & clip(MyData),'Should be on "Jones"')
                         END
                         XML:GotoTop
                         if not xml:FindNextContent('Dunninghurst',False,False)
                            message('FindNextContent successful:  Found "Dunninghust"','FindNextContent',icon:asterisk)
                         else
                            message('FindNextContent did not work successful','FindNextContent',icon:exclamation)
                         END
                      end
                   end
                end
             end
          end
       end


       Xml:Free()
    End


ShowXMLError    ROUTINE

    message('An error occurred in the XML Parser!||' & XML:GetError(),'Error!',icon:exclamation)

[END]
