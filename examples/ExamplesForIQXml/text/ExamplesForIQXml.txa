EMBED %ProcedureInitialize
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
    ! This routine will add an element prefix of "QueueData" for what is created within the queue

    ! Add a customer with 2 invoices
    ! ------------------------------
    Lastname='Prior'
    Firstname='Maddy'
    StreetName[1]='123 Main Street'
    StreetName[2]='PO Box 123'
    City='Wethersfield'
    State='CT'
    Zipcode='06109'
    Country='USA'
    InvoiceAmt[1]='100.00'
    InvoiceDate[1]='2005-04-01'
    InvoiceNo[1] = '1'
    InvoiceAmt[2]='200.00'
    InvoiceDate[2]='2005-04-02'
    InvoiceNo[2] = '2'
    add(MyQueue)

    ! Add another customer with 20 random invoices
    ! --------------------------------------------
    clear(MyQueue)
    Lastname='Woods'
    Firstname='Gaye'
    StreetName[1]='55 Bedlam Born'
    StreetName[2]=''
    City='Newington'
    State='CT'
    Zipcode='06111'
    Country='USA'
    loop i = 1 to 20
        InvoiceAmt[i]=random(100.00,300.99)
        InvoiceDate[i]='2005-04-' & format(random(1,30),@n02)
        InvoiceNo[i] = random(100,3000)
    end
    add(MyQueue)

   ! Write out as XML file
   ! ---------------------
   if ~XML:CreateXMLFILE('Work.XML','1.0','ISO-8859-1',1)

        XML:SetElementPrefix('iQXML')

        XML:CreateParent('Example')
        XML:CreateAttribute('xmlns:iQXML','http://mycompany.com/namespaces/2005')
        XML:AddParent()

        XML:CreateParent('Customers')
        XML:CreateAttribute('Company','InnQuest Software')
        XML:CreateAttribute('RunDate',format(today(),@d010-))
        XML:AddParent()


        XML:AddfromQueue(MyQueue,'Customer',1)

        xml:CloseParent()

        xml:CloseParent()

        XML:SetElementPrefix('')

        xml:CLoseXMLFILE()
    ELSE
       MESSAGE('An error occurred creating XML file.||Error: ' & error(),'Error',icon:exclamation)
    end
[END]
