LoadXML ROUTINE

  ! Load the XML file to process it
  ! -------------------------------
  If ~Xml:LoadFromFile('AddressExample.xml',False)
      XML:QualifyFieldSet('ID')
      if XML:LoadQueue(CustomerQueue,True,True).
      XML:PrimaryKeyCascade('CustomerID')
      XML:GotoTop()
      if XML:LoadQueue(InvoiceQueue,True,True).
      XML:Free()
   END
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
SyncQueue   ROUTINE

  GET(CustomerQueue,CHOICE(?LIST1))

  FREE(ShowInvoices)
  LOOP I = 1 TO RECORDS(InvoiceQueue)
    GET(InvoiceQueue,i)
    if IQ:CustomerID = CQ:CustomerID
       SI:InvoiceNo = IQ:InvoiceNo
       SI:InvoiceDate = IQ:InvoiceDate
       SI:InvoiceAmt = IQ:InvoiceAmt
       ADD(ShowInvoices)
    END
  END

  DISPLAY()
[END]
