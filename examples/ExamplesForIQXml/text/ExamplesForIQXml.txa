   parstring = parstring & 'Elaine,'
   parstring = parstring & 'JoAnn,'
   parstring = parstring & 'Jim,'
   parstring = parstring & 'Bob,'
   parstring = parstring & '''Dog'','
   parstring = parstring & '''Cat'','
   parstring = parstring & '''Fish''''s'','
   parstring = parstring & ','                ! blank one
   parstring = parstring & ''''','            ! blank two
   parstring = parstring & 'Two Blanks Above,'
   parstring = parstring & ''','''            ! and a comma
   message(parstring,'StartString')
   loop xml:parse(ParString,',','''') TIMES
       FullString = FullString & xml:parseread() & '|'
       i+=1
   end
   message(FullString,'Names',icon:asterisk)
   xml:parseempty()
   if i <> 12 then message('Error in extraction! - Counted ' & i,'Error!',icon:hand).

