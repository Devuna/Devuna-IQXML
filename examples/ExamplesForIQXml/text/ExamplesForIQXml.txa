WriteXML    ROUTINE

    if not XML:CreateXMLString('1.0','UTF-8',True)
       XML:AddChild('Songs')
       XML:CreateParent('Song')
       XML:CreateAttribute('Quantity','3')
       XML:CreateAttribute('Type','Blind')
       XML:AddParent(True,'Mice')
       XML:CreateParent('Song')
       XML:CreateAttribute('Quantity','10')
       XML:CreateAttribute('Type','Little')
       XML:AddParent(True,'Indians')
       XML:CreateParent('Song')
       XML:CreateAttribute('Quantity','12')
       XML:CreateAttribute('Type','Days')
       XML:AddParent(True,'Christmas')
       XML:CloseParent()
       UseString = XML:CloseXMLString()

       message(UseString,'Display XML File')

    end

[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
ReadXML ROUTINE

    IF NOT XML:LoadFromString(useString)

       ! EXTRACT FIRST COMMENT OUT OF XML
       if XML:ReadComment(1,ShowComment).
       message('The comment in the file is:||' & ShowComment,'Test Comment')

       ! Show Each of the three songs
       if XML:LoadQueue(SongQueue,True,True).

       XML:DebugMyQueue(SongQueue)

       XML:FREE()
    END
