EMBED %ProcessedCode
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END

   ! -------------------------------------------------------
   ! Look at Group Structure, External Name parameters.
   ! This allows some excellent flexibility when
   ! writing out a group to an XML file in iQ-XML.
   !
   ! Example : This allows the fields from the TPS file to be
   ! changed automatically when writing out to the XML file,
   ! and also allowed a MixedCase format as well.
   !
   ! Notice that the field UNIFORMNUMBER is in the database
   ! however, it is written to the XML file as "Uniform"
   !
   ! -------------------------------------------------------

   ! without ADDCHILD()
   IF NOT XML:CreateXMLFile('Work.XML',,,True)
        SETCURSOR(CURSOR:WAIT)
        XML:CreateParent('BenchMark')
        XML:CreateAttribute('Date',format(today(),@d010-))
        XML:CreateAttribute('Time',format(clock(),@t07))
        XML:AddParent()

        xml:createParent('Detail')
        XML:CreateAttribute('Date',format(today(),@d010-))
        XML:CreateAttribute('Time',format(clock(),@t07))
        XML:AddParent()
        loop j = 1 to 60000
           xml:CreateParent('Testing');xml:AddParent()
           xml:AddElement('Line','Information for Line: ' & J)
           xml:CloseParent()
        end
        xml:CloseParent()
        xml:CloseParent()

        xml:CloseXMLFile()
        SETCURSOR()
        message('The XML file (Work.XML) using AddParent() completed in ' & XML:GetTime(),'0.55 is a normal Bench Mark',icon:asterisk)

   END

   ! with ADDCHILD()
   IF NOT XML:CreateXMLFile('Work.XML',,,True)
        SETCURSOR(CURSOR:WAIT)
        XML:CreateParent('BenchMark')
        XML:CreateAttribute('Date',format(today(),@d010-))
        XML:CreateAttribute('Time',format(clock(),@t07))
        XML:AddParent()

        xml:createParent('Detail')
        XML:CreateAttribute('Date',format(today(),@d010-))
        XML:CreateAttribute('Time',format(clock(),@t07))
        XML:AddParent()
        loop j = 1 to 60000
           xml:AddChild('Testing')
           xml:AddElement('Line','Information for Line: ' & J)
           xml:CloseParent()
        end
        xml:CloseParent()
        xml:CloseParent()

        xml:CloseXMLFile()
        SETCURSOR()
        message('The XML file (Work.XML) using AddChild() completed in ' & XML:GetTime(),'0.50 is a normal Bench Mark',icon:asterisk)

    END
[END]
[END]
