WHEN '?GetBigFile'
[INSTANCES]
WHEN 'Accepted'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END

    bigFile = sa:readFromfile('ABC.xml')

    if errorcode()
       message('Error Occurred opening abc.XML file.||Error: ' & ERROR(),'Error Occurred',icon:exclamation)
    else
       REMOVE('ABC2.XML')
       BigFileLen = len(BigFile)
       MESSAGE('File was read as ' & sa:formatfilesize(BigFileLen,'.'),'SA:ReadFromFile',icon:asterisk)
       SA:WriteToFileA('ABC2.XML',BigFile)
       MESSAGE('File was written as ' & sa:formatfilesize(SA:GetFileInfo('abc2.xml',4),'.'),'SA:WriteFileA',icon:asterisk)
    end
[END]
