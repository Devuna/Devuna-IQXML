EMBED %ProcedureInitialize
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
    Do FillTable

    XML:SetNoIndent(True)

    ! any attributes created before CreateXML are added to the <xml tag>
    xml:CreateAttribute('standalone','yes')

    EXECUTE InType
    ReturnValue = XML:CreateXMLFILE('Work.XML','1.0','ISO-8859-1',,'')
    ReturnValue = XML:CreateXMLString('1.0','ISO-8859-1',,'')
    END

    IF ReturnValue <> 0
       MESSAGE('An error occurred creating XML file.||Error: ' & error(),'Error',icon:exclamation)
       DO ProcedureReturn
    END

    xml:CreateParent('Response')
    XML:CreateAttribute('TimeStamp',IQXML:ISO8601GMT)
    XML:AddParent()

    XML:CreateParent('CD')
    XML:CreateAttribute('serial','B909098')
    XML:CreateAttribute('disc-length','49:41')
    XML:AddParent()

    XML:AddElement('Artist','Steeleye Span')
    XML:AddElement('Title','Bedlam Born')
    XML:AddElement('Genre','Folk')
    XML:AddElement('Released','2000')

    XML:AddComment('Steeleye Span is one of the best Folk bands around and have been playing for over 35 years!  Learn about them on the web! http://steeleye.freeservers.com/')

    XML:CreateParent('Songs');XML:AddParent()

    LOOP I = 1 TO RECORDS(Album)
        GET(Album,I)
        XML:CreateParent('Song');XML:AddParent()
        XML:AddElement('Title',que:title)
        XML:AddElement('Length',que:length)
        xml:CloseParent('Song')
    END

    xml:CloseParent('Songs')   ! <songs>
    xml:CloseParent('CD')   ! <cd>
    xml:CloseParent('Response')

    xml:AddStringText(chr(60) & '!-- This is a comment added to end of document using XML:AddStringTxt --' & chr(62))

    EXECUTE InType
    xml:CloseXMLFILE()
    ReturnXMLData = xml:CloseXMLString(True)
    END

[END]
