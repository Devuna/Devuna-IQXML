WHEN 'Accepted'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
  case SA:DoesFileContain(InFileName,InFileText)
  of False
     message(clip(InFileName) & ' does not contain ' & infiletext,'Nope!')
  of True
     message(clip(InFileName) & ' really does contain ' & infiletext,'Yup!')
  of 3
     message(clip(InFileName) & ' does not exists.||' & ERROR(),'Error!')
  end
