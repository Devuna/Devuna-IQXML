[PROCEDURE]
NAME XMLPrimaryExtended
[COMMON]
FROM Clarion Source
MODIFIED '2009/10/16' '16:58:38'
[PROMPTS]
%GenerateOpenClose LONG  (0)
[EMBED]
EMBED %ProcessedCode
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
    If ~xml:LoadFromFile('PrimaryKeyExtended.xml')
       If xml:FindNextNode('Customers','Customer')
            Do ShowXMLError
       ELSE
            xml:QualifyFieldSet('id')
            xml:QualifyFieldSet('Name',True)
            xml:QualifyFieldSet('Line1')
            xml:QualifyFieldSet('Line2')

            xml:QualifyFieldSet('Transport')
            xml:QualifyFieldSet('Number')
            xml:QualifyFieldSet('date')
            xml:QualifyFieldSet('Amt')


            xml:PrimaryKeyCascade('TruckingTransport',IQXML:CASCADE_SAVE)             ! Keeps the value for all inserted records
            xml:PrimaryKeyCascade('CustomerID')
            xml:PrimaryKeyCascade('CustomerHeadername')
            xml:PrimaryKeyCascade('AddressLine1')
            xml:PrimaryKeyCascade('AddressLine2')

            xml:AutoRootSet()                                                         ! Needed since two LoadQueues will run, each from the top
            IRecs = XML:LoadQueue(InvoicesQueue,True,True)                            ! Load in all the invoice records
            xml:debugqueue('After Loading Invoices')

            xml:QualifyFieldSet('Line')
            xml:PrimaryKeyCascade('InvoiceNumber')
            xml:PrimaryKeyCascade('NoteID',IQXML:CASCADE_PARENT)                      ! Does not cascade once it hits new parent
            XML:AttributeFieldSet('NoteLine',IQXML:FORCEWRITE + IQXML:Required)       ! Forces new/nob-blank records for every note line

            IRecs = XML:LoadQueue(NotesQueue,True,True)                               ! Load in all the notes

            xml:AutoRootClear()

            xml:DebugMyQueue(InvoicesQueue,'Invoices Queue')                          ! Show the invoices
            xml:DebugMyQueue(NotesQueue,'Notes Queue')                                ! Show the notes

            xml:Free()                                                                ! also clears primary keys

        END
     ELSE
        MESSAGE('Error occurred loading file.||Error: ' & ERROR(),'Error!',icon:exclamation)
     END


ShowXMLError    ROUTINE

    MESSAGE('An error occurred in the XML Parser!||' & XML:GetError(),'Error!',icon:exclamation)



[END]
