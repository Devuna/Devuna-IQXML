  CLEAR(JySalWerk)
  JySalWerk.Name = 'Robert'
  JySalWerk.Surname = 'Paresi'
  JySalWerk.DateOfBirth = DEFORMAT('11/18/2000',@d02b)  ! mm/dd/yyyy to Clarion Format
  JySalWerk.HSGRADDATE = DEFORMAT('11/18/1986',@d02b)  ! mm/dd/yyyy to Clarion Format
  JySalWerk.UNGRADDATE = DEFORMAT('11/18/1990',@d02b)  ! mm/dd/yyyy to Clarion Format

  IF NOT XML:CreateXMLString('1.0','UTF-8',FALSE)
    XML:SetFieldPicture('DATEOFBIRTH','@d010-')
    XML:AddFromGroup(JySalWerk,'TestGroup')
    MESSAGE('Only the first date should be in ISO format:||' & XML:CloseXMLString(),'Format Example',icon:asterisk)
    XML:SetFieldPicture('DATEOFBIRTH','')
  END

  CLEAR(JySalWerk)
  JySalWerk.Name = 'Robert'
  JySalWerk.Surname = 'Paresi'
  JySalWerk.DateOfBirth = DEFORMAT('11/18/1970',@d02b)  ! mm/dd/yyyy to Clarion Format
  JySalWerk.HSGRADDATE = DEFORMAT('11/18/1986',@d02b)  ! mm/dd/yyyy to Clarion Format
  JySalWerk.UNGRADDATE = DEFORMAT('11/18/1990',@d02b)  ! mm/dd/yyyy to Clarion Format

  IF NOT XML:CreateXMLString('1.0','UTF-8',FALSE)
    XML:SetFieldPicture('*DATE*','@d010-')
    XML:AddFromGroup(JySalWerk,'TestGroup')
    MESSAGE('All three following dates should be in ISO format:||' & XML:CloseXMLString(),'Format Example',icon:asterisk)
