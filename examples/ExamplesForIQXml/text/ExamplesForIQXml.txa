EMBED %ProcessedCode
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
  ! -------------------------------------------------------
  ! The XML:GetSoapContent function extracts SOAP information
  ! quickly and easily without using to use a queue, or
  ! navigate through the xml data at all.
  !
  ! The node is in the form of field.field.field, and
  ! does not include any of the SOAP nodes.
  ! -------------------------------------------------------

  if NOT XML:LoadFromFile('SoapExample.xml')

        if xml:GetSoapContent('MyRootTag.CustomerTag.LastName',LastName).
        if xml:GetSoapContent('MyRootTag.CustomerTag.Firstname',FirstName).
        if xml:GetSoapContent('MyRootTag.CustomerTag.CustID',CustID).

        MESSAGE('The Soap Envelope shows ...||FirstName: ' & FirstName & '||LastName: ' & LastName & '||CustID: ' & CustID,'Soap Example',icon:asterisk)

        xml:FREE()

   end

[END]
[END]
