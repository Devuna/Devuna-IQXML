   ! retains inside and removes outside

   s1 = 'SQL(''SELECT * FROM FILE WHERE (THIS = THAT AND THAT <> THIS) ORDER BY THIS'')'
   ResultString = SA:ExtractOutside(s1,'(',')','B',True)

   MESSAGE('SQL wrapper only is||' & ResultString)

[END]
[END]
WHEN '?ExtractInsideWide:2'
