[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
  XML:BinaryFieldSet('BinaryFile')  ! Tell IQXML that <BinaryFile> is containing an object
                                    !  so store the contents as an object and the object name as the field instead

  IF NOT XML:LoadFromFile(XMLFILE)  ! Load the XML file storing the binary objects

     IF message('Do you want to see the debug screen and how it is parsed?','See Debug?',icon:question,button:YES+button:NO) = button:YES
        xml:DebugQueue('After Parsing')
     END

     if XML:FindNextNode('BinaryFile')  ! find the binary files and load them in our queue
        message('Error occurred and this does not seem to be a BinaryFile created by XML.','Node Not Found',icon:exclamation)
     ELSE
        Recs = XML:LoadQueue(BinaryQueue,True,True)
        message('We found ' & RECS & ' binary object(s) <BinaryFile> in this XML file.','Objects Found',icon:asterisk)

        ! store the file names as (the file name you choose).X
        !
        ! if this were a real application, we would store the files as the original file names passed by the
        ! original vendor, or we can just call them anything we want and put them where we want.
        !
        ! We can't keep them as the work file names (echotoken.xmlobj) because XML:FREE will remove them
        ! ------------------------------------------------------------------------------------------------
        if Recs > 0
           LOOP I = 1 TO RECS
             GET(BinaryQueue,i)
             COPY(QUE:BinaryFile,clip(LocalBinaryFile) & '.' & I)
             if errorcode()
                message('Could not create the binary object file.||In: ' & CLIP(QUE:BinaryFile) & '||Error: ' & ERROR(),'Error Occurred',icon:exclamation)
             ELSE
                MESSAGE('iQ-XML has created the binary object file.||File: ' & clip(LocalBinaryFile) & '.' & I,'Completed',icon:asterisk)
             END
           END
        END
     END
  ELSE
     MESSAGE('File was not found.||Error: ' & ERROR(),'Error',icon:exclamation)
  END

  XML:FREE()                ! the .xmlobj files (temporary objects are automatically deleted when XML:FREE is called)
  FREE(BinaryQueue)         ! CLEAR OUR LOCAL QUEUE
[END]
[END]
