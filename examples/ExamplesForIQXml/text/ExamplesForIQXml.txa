EMBED %ControlPreEventHandling
[INSTANCES]
WHEN '?ExtractOutside'
[INSTANCES]
WHEN 'Accepted'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
   ! retains everything but between

   s1 = '(Removes Record 2)||!001This is Record One!002This is record two.!003This is record three'
   ResultString = SA:ExtractOutside(s1,'!002','!','R')

   MESSAGE(ResultString)


   s1 = '(Removes Record 1)||!001This is Record One!002This is record two.!003This is record three'
   ResultString = SA:ExtractOutside(s1,'!001','!','R')

   MESSAGE(ResultString)
[END]
[END]
WHEN '?ExtractInside'
[INSTANCES]
WHEN 'Accepted'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
   ! retains inside and removes outside

   s1 = '!001This is Record One!002This is record two.!003This is record three'
   ResultString = SA:ExtractInside(s1,'!002','!')

   MESSAGE('(Shows Record 2)||' & ResultString)


   s1 = '!001This is Record One!002This is record two.!003This is record three'
   ResultString = SA:ExtractInside(s1,'!001','!')

   MESSAGE('(Shows Record 1)||' & ResultString)


   s1 = '!001This is Record One!002This is record two.!003This is record three'
   ResultString = SA:ExtractInside(s1 & '!','!003','!')

   MESSAGE('(Shows Record 3)||' & ResultString)
[END]
[END]
WHEN '?ExtractInsideWide'
[INSTANCES]
WHEN 'Accepted'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
   ! retains inside and removes outside

   s1 = 'SQL(''SELECT * FROM FILE WHERE (THIS = THAT AND THAT <> THIS) ORDER BY THIS'')'
   ResultString = SA:ExtractInside(s1,'(',')','',True)

   MESSAGE('SQL statement really is||' & ResultString)

[END]
[END]
WHEN '?ExtractOutsideWide'
[INSTANCES]
WHEN 'Accepted'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
