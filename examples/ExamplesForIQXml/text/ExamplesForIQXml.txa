EMBED %ProcessedCode
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
  ! Load the XML file to process it
  ! -------------------------------
  If ~Xml:LoadFromFile('AddressExample.xml',False)

      ! Find and position to the first parent node
      ! ------------------------------------------
        If xml:FindNextNode('Customers','Customer')
           Do ShowXMLError
        ELSE
          LOOP
            CLEAR(Customerqueue)
            FREE(Customerqueue)
            CLEAR(Invoicequeue)
            FREE(Invoicequeue)

            ! mark this as the top of the "Customer" and always come back here after each LoadQueue function
            ! consider this the "Customer BLock of Information"
            ! -------------------------------------------------
            XML:AutoRootSet()

            ! read the customer header information
            CRecs = XML:LoadQueue(CustomerQueue,True,False)

            ! --------------------------------------------------------------
            ! Now read all the invoices.  It is important in this case
            ! to select the ALL siblings flag to include all "Invoice" records.
            ! --------------------------------------------------------------
            If ~xml:FindNextNode('Invoices','Invoice')
                IRecs = XML:LoadQueue(InvoiceQueue,True,True)
            End

            ! Show what was loaded
            ! --------------------
            XML:DebugMyQueue(CustomerQueue,'Customer Queue')
            XML:DebugMyQueue(InvoiceQueue,'Invoice Queue')

            ! now go to next customer - break out if there isn't one.
            ! -------------------------------------------------------
            If Xml:GotoSibling() then break.
        End
       End

       Xml:Free()
    End


