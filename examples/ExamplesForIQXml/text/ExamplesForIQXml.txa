WHEN '?GetSmallFile'
[INSTANCES]
WHEN 'Accepted'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
    case popup('Write Text to File|Write Text Dump to File|Write some lines easier this way!|-|Display File')
    of 1
        SA:WriteToFile('debug.log','This is a test line',False,2)
    of 2
        SA:WriteToFile('debug.log','<- This is debug information ->',True,2)
        SA:WriteToFile('debug.log','This is a test line',True,20)
    of 3
        SA:WriteToFileSetName('debug.log',True,2)

        SA:writeline('Hello')
        SA:writeline('Easy Write')
        SA:writeline('This is easier to do')
        SA:writeline('Just set the debug file name and attributes at the beginning of your program!')
        SA:writeline(' - then just use WriteLine')
   else
       XML:ViewFile('debug.log')
   end


[END]
[END]
